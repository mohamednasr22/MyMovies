@model Vidly.Models.Movie

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.TypeMovie.Name</li>
    <li>Release Date: @Model.DateReleased.ToLongDateString()</li>
    <li>Date Added: @Model.DateAdd.ToLongDateString()</li>
    <li>Number in Stock: @Model.Quantdy</li>
</ul>